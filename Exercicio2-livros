
package Produtos;


    public class Livro{
        private String nome;
        private double preco;
        private String autor;
    
    
        public void Livro(String nome, double preco, String autor) {
            this.nome = nome;
            this.preco = preco;
            this.autor = autor;
             

        }

        @Override
        public String toString() {
            return "Nome: "+nome + "\npreco: " + preco + "\nautor: " + autor;
        }
    }
 
